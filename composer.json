{
    "name": "pdepend/pdepend",
    "description": "Official version of pdepend to be handled with Composer",
    "license": "BSD-3-Clause",
    "version": "2.0.0",
    "require": {
        "php": ">=5.3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    },
    "bin": ["src/bin/pdepend"],
    "autoload": {
        "psr-0": {"PHP_": "src/main/php/"}
    }
}

